<br>
<h3 class="text-center">
  My Wishlist <i class="far fa-heart"></i>
</h3>
<hr>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @wishlists.each do |wishlist| %>
      <tr>
        <td><%= wishlist.product.name %></td>
        <td><%= wishlist.product.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>
